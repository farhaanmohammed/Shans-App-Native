{
  "expo": {
    "name": "shansapp",
    "slug": "shansapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "flex": 1,
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "buildGradle":{
        "configurations":{
          "compileOnly":{
            "arguments":  ["-Xlint:deprecation"]
          }
        }
      },
      
      "package": "com.shansapp.android"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "9ef1e5ec-cd9a-4b0e-8163-22e8cec3ffd7"
      }
    },
    "owner": "shansapp"
  }
}
